/dts-v1/;
/plugin/;

#include "cape-sde-display-oneli-common.dtsi"

&sde_te1_active {
	mux {
		pins = "gpio124";
		function = "mdp_vsync";
	};

	config {
		pins = "gpio124";
		drive-strength = <2>;   /* 2 mA */
		bias-pull-down;         /* PULL DOWN */
	};
};

&sde_te1_suspend {
	mux {
		pins = "gpio124";
		function = "mdp_vsync";
	};

	config {
		pins = "gpio124";
		drive-strength = <2>;   /* 2 mA */
		bias-pull-down;         /* PULL DOWN */
	};
};

&soc {
	vci_disp_m_vreg:  vci_disp_m_vreg{
		compatible = "regulator-fixed";
		regulator-name = "vci_disp_m_supply";
		regulator-min-microvolt = <3000000>;
		regulator-max-microvolt = <3000000>;
		enable-active-high;
		regulator-boot-on;

		gpio = <&tlmm 107 0>;
		pinctrl-names = "default";
		pinctrl-0 = <&vci_en_m_default>;
		vin-supply = <&L13C>;
	};

};

&sde_dsi1 {
	qcom,platform-te-gpio = <&tlmm 124 0>;
};

&mot_boe_269_800x600_cmd {
	qcom,mdss-dsi-te-using-wd;
};


/ {
	model = "oneli";
	compatible = "qcom,cape-mtp", "qcom,cape", "qcom,mtp", "moto,oneli";
	qcom,msm-id = <530 0x10000>, <531 0x10000>, <540 0x10000>;
	qcom,board-id = <0x45 0xA100>;
};
